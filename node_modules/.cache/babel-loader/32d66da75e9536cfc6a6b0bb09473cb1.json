,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA,CADA,CAEA;;AACA,WAAA,IAAA,GAAA,MAAA;AACA,KAlBA;AAmBA,IAAA,UAAA,EAAA,sBACA;AAAA;;AACA,UAAA,KAAA,IAAA,KAAA,EAAA,EACA;AACA,QAAA,KAAA,CAAA,gCAAA,CAAA;AACA;AAEA,OANA,CAQA;;;AAEA,UAAA,MAAA,GAAA,EAAA;;AACA,UAAA,KAAA,KAAA,IAAA,IAAA,EACA;AACA,YAAA,CAAA,GAAA,CAAA;AACA,aAAA,KAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EACA;AACA,UAAA,CAAA;AACA,UAAA,MAAA,GAAA,MAAA,GAAA,KAAA;;AACA,cAAA,CAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,EACA;AACA,YAAA,MAAA,IAAA,GAAA;AAEA;AACA,SATA;AAUA,OAbA,MAeA;AACA,QAAA,MAAA,GAAA,EAAA;AACA;;AACA,WAAA,OAAA,GAAA,MAAA;AAEA,UAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,KAAA,IAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,KAAA,QAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,KAAA,SAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,SAAA,EAAA,KAAA,OAAA;AAGA,WAAA,KAAA,CAAA,IAAA,CAAA,KAAA,IAAA,EAAA,QAAA,EAEA;AACA;AACA,QAAA,OAAA,EAAA;AACA,0BAAA;AADA;AADA,OAHA,EAQA,IARA,CAQA,UAAA,QAAA,EACA;AACA,QAAA,KAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA;AAEA,OAZA,EAaA,KAbA,CAaA,UAAA,KAAA,EACA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAhBA;AAmBA;AA7EA;AAvBA,CAAA","sourcesContent":["<template>\r\n<div>\r\n<!-- Page Content -->\r\n<div class=\"container\">\r\n  <!-- <div class=\"card border-0 shadow my-5\">\r\n    <div class=\"card-body p-5\"> -->\r\n       <b-alert show variant=\"warning\">Nb: Please convert your file to csv first before uploading</b-alert>\r\n<div class=\"dropfile\">\r\n <input \r\n      type=\"file\"\r\n      class=\"fileinput\"      \r\n      ref=\"file\"\r\n      @change=\"onChange\">\r\n     \r\n<h2 class=\"inputtext\">Drop csv file here</h2>\r\n\r\n</div>\r\n\r\n<div class=\"row\">\r\n<div class=\"col-sm-4\">\r\n  Target Type \r\n<b-form-select placeholder=\"Target type\" v-model=\"selected\" :options=\"options\">\r\n    -- Please select an target type --\r\n</b-form-select>\r\n  </div>\r\n   <div class=\"col-sm-4\">\r\n     Your table name \r\n      <b-form-input v-model=\"tablename\" placeholder=\"tablename\"></b-form-input>\r\n      \r\n    <!-- <div class=\"mt-2\">Defined table <b>{{ tablename }}</b></div> -->\r\n  </div>\r\n  <div class=\"col-sm-4\">\r\n   <!-- Your Columns (add , after the name)\r\n      <b-form-input v-model=\"columns\" placeholder=\"target table column names\"></b-form-input>\r\n    <div class=\"mt-2\"><b>{{ columns }}</b></div>-->\r\n     \r\n\r\n   Enter column names and press enter\r\n    <b-form-tags \r\n    input-id=\"tags-basic\" \r\n    separator=\" ,;\" \r\n    placeholder=\"add column name\" \r\n    v-model=\"value\" \r\n    class=\"mb-3\"\r\n    ></b-form-tags>\r\n\r\n  </div>\r\n\r\n    <div class=\"col-sm-4\">\r\n      <b-button \r\n      size=\"md\"       \r\n    variant=\"primary\"\r\n      @click=\"uploadFile()\">Convert File</b-button>\r\n  </div>\r\n \r\n</div>\r\n<div>\r\n    <br>\r\n  <b-form-textarea\r\n    id=\"textarea-rows\"\r\n    placeholder=\"Converted output\"\r\n    rows=\"8\"\r\n    style=\"font-family:monospace;\"\r\n    v-model=\"convertedData\"    \r\n  ></b-form-textarea>\r\n\r\n  <div class=\"mt-3\">\r\n    <b-button-group size=\"sm\">\r\n      <b-button @success=\"handleSuccess\"  @error=\"handleError\" v-clipboard=\"convertedData\">Copy</b-button>\r\n      <b-button @click=\"clear()\">Clear</b-button>\r\n    </b-button-group>\r\n  </div>\r\n</div>\r\n\r\n  </div>\r\n\r\n<!-- </div>\r\n</div>   -->\r\n\r\n<Footer></Footer>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport Footer from './Footer.vue'\r\nexport default {\r\n  name: 'spreadsheet',\r\n  components: {\r\n    Footer\r\n   \r\n},\r\n  data()\r\n  {\r\n    return {\r\n       //  http:'http://localhost/csvadapter-api/index.php',\r\n       http:'https://api.csvadapter.com/index.php',\r\n        file:'',\r\n        selected:'SQL',\r\n        options:['SQL','MySQL','MsSQL','PostgreSQL','SQLite'],\r\n        convertedData:'',\r\n        name:'SQL and MySQL',\r\n        tablename:'',\r\n        columns:'',\r\n        value: [],\r\n        query:''\r\n    }\r\n  },\r\n  methods:\r\n  {\r\n    handleSuccess() \r\n    {\r\n      alert(\"Successfully Copied Your Query!\");\r\n    },\r\n    handleError(e) {\r\n            window.console.log(e);\r\n        },\r\n    clear()\r\n    {\r\n      this.convertedData='';\r\n\r\n    },\r\n      onChange($event)\r\n      {\r\n        var myfile = $event.target.files[0];        \r\n        //read file contents\r\n        this.file= myfile;    \r\n      },\r\n      uploadFile:function()\r\n      {   \r\n        if(this.file==='')\r\n        {\r\n          alert('You cannot convert empty file ');\r\n          return;\r\n\r\n        }\r\n        \r\n        //check if they have supplied table name\r\n\r\n        var column='';\r\n        if(this.value!=null)\r\n        {\r\n          var i=0;\r\n          this.value.forEach((value)=>\r\n          {\r\n            i++;\r\n            column = column + value;\r\n            if(i<this.value.length)\r\n            {\r\n               column +=\",\";          \r\n\r\n            }\r\n          });      \r\n        }\r\n        else\r\n        {\r\n          column='';\r\n        }\r\n        this.columns = column;\r\n\r\n       let formData = new FormData();\r\n       formData.append('file', this.file);\r\n       formData.append('type',this.selected);\r\n       formData.append('table',this.tablename);\r\n       formData.append('columns',this.columns);\r\n    \r\n    \r\n    this.$http.post(this.http, formData,\r\n      \r\n      // this.$http.post('../php/index.php', formData,\r\n       {\r\n          headers:{\r\n            'Content-Type': 'multipart/form-data'\r\n          }\r\n       })\r\n       .then((response)=>\r\n       {  \r\n           this.convertedData = response.data;\r\n           \r\n       })\r\n       .catch(function (error)\r\n       {\r\n           window.console.log(error);\r\n       });\r\n       \r\n\r\n     }\r\n     \r\n  } \r\n\r\n}\r\n\r\n</script>\r\n\r\n<style> \r\n/* Set a background image by replacing the URL below */\r\nbody {\r\n  /*background: url('https://www.excel-easy.com/basics/images/worksheets/sheet1.png');*/\r\n  -webkit-background-size: cover;\r\n  -moz-background-size: cover;\r\n  background-size: cover;\r\n  -o-background-size: cover;\r\n  background-color: white;\r\n}\r\n.dropfile{\r\n    background: whitesmoke;\r\n    height: 100px;\r\n    border: 2px dashed rgb(204, 228, 228);\r\n    outline-offset: -10px;\r\n    font-family: sans-serif;\r\n    margin: 20px auto;\r\n   \r\n    position: relative;\r\n\r\n}\r\n.fileinput{\r\nheight: 100px; width:100%;\r\nposition: absolute;\r\nopacity:0;\r\ntext-align: center;\r\n}\r\n.inputtext{\r\ntext-align: center;\r\nfont-family: \"Sans-serif\", Arial, Verdana;\r\nmargin: 30px auto;\r\n\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}